<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="SwitchView">
        <attr name="textTrue" format="string" />
        <attr name="textFalse" format="string" />
        <attr name="container_Hight" format="dimension" />
        <attr name="container_Width" format="dimension" />
        <attr name="cursor_Hight" format="dimension" />
        <attr name="cursor_Width" format="dimension" />
        <attr name="cursor_Background" format="reference|color" />
        <attr name="container_Background" format="reference|color" />
    </declare-styleable>
    <attr name="mwhWaves" format="string|enum">
        <enum name="MultiWave" value="-1"/>
        <enum name="PairWave" value="-2"/>
    </attr>
    <declare-styleable name="MultiWaveHeader">
        <attr name="mwhWaveHeight" format="dimension"/>
        <attr name="mwhStartColor" format="color"/>
        <attr name="mwhCloseColor" format="color"/>
        <attr name="mwhIsRunning" format="boolean"/>
        <attr name="mwhColorAlpha" format="fraction|float"/>
        <attr name="mwhProgress" format="fraction|float"/>
        <attr name="mwhVelocity" format="fraction|float"/>
        <attr name="mwhGradientAngle" format="integer"/>
        <attr name="mwhWaves"/>
    </declare-styleable>

    <!--<declare-styleable name="Wave">-->
        <!--<attr name="mwOffsetX" format="dimension"/>-->
        <!--<attr name="mwOffsetY" format="dimension"/>-->
        <!--<attr name="mwVelocity" format="dimension"/>-->
        <!--<attr name="mwWaveHeight" format="dimension"/>-->
        <!--<attr name="mwScaleX" format="float"/>-->
        <!--<attr name="mwScaleY" format="float"/>-->
    <!--</declare-styleable>-->



    <attr name="item_count" format="integer"></attr>
    <declare-styleable name="ViewPagerIndicator01">
        <attr name="item_count" />
    </declare-styleable>

    <declare-styleable name="MyRatingBar">
        <attr name="base_of_width_or_height" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
        <attr name="step" format="float"/>
        <attr name="rating" format="float"/>
        <attr name="stars" format="integer"/>
    </declare-styleable>
    <!--popupbtn-->
    <declare-styleable name="popupbtn">
        <!-- 正常状态下的背景 -->
        <attr name="normalBg" format="reference" />
        <!-- 按下状态下的背景 -->
        <attr name="pressBg" format="reference" />
        <!-- 正常状态下的图标 -->
        <attr name="normalIcon" format="reference" />
        <!-- 按下状态下的图标 -->
        <attr name="pressIcon" format="reference" />
        <!-- 按钮的padding -->

    </declare-styleable>
    <declare-styleable name="LCardView">

        <attr name="leftShadowWidth" format="dimension"/>
        <attr name="topShadowHeight" format="dimension"/>
        <attr name="rightShadowWidth" format="dimension"/>
        <attr name="bottomShadowHeight" format="dimension"/>
        <attr name="shadowSize" format="dimension"/>
        <attr name="shadowColor" format="color"/>
        <attr name="shadowStartAlpha" format="integer"/>
        <attr name="shadowFluidShape" format="enum">
            <enum name="adsorption" value="0"/>
            <enum name="linear" value="1"/>
        </attr>
        <attr name="cardBackgroundColor" format="color"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="leftTopCornerRadius" format="dimension"/>
        <attr name="rightTopCornerRadius" format="dimension"/>
        <attr name="leftBottomCornerRadius" format="dimension"/>
        <attr name="rightBottomCornerRadius" format="dimension"/>
        <attr name="elevation" format="dimension"/>
        <attr name="elevationAffectShadowColor" format="boolean"/>
        <attr name="elevationAffectShadowSize" format="boolean"/>
        <attr name="xOffset" format="dimension"/>
        <attr name="yOffset" format="dimension"/>
    </declare-styleable>

</resources>