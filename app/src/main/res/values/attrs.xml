<?xml version="1.0" encoding="utf-8"?>
<resources>



    <attr name="mwhWaves" format="string|enum">
        <enum name="MultiWave" value="-1"/>
        <enum name="PairWave" value="-2"/>
    </attr>
    <declare-styleable name="MultiWaveHeader">
        <attr name="mwhWaveHeight" format="dimension"/>
        <attr name="mwhStartColor" format="color"/>
        <attr name="mwhCloseColor" format="color"/>
        <attr name="mwhIsRunning" format="boolean"/>
        <attr name="mwhColorAlpha" format="fraction|float"/>
        <attr name="mwhProgress" format="fraction|float"/>
        <attr name="mwhVelocity" format="fraction|float"/>
        <attr name="mwhGradientAngle" format="integer"/>
        <attr name="mwhWaves"/>
    </declare-styleable>

    <!--<declare-styleable name="Wave">-->
        <!--<attr name="mwOffsetX" format="dimension"/>-->
        <!--<attr name="mwOffsetY" format="dimension"/>-->
        <!--<attr name="mwVelocity" format="dimension"/>-->
        <!--<attr name="mwWaveHeight" format="dimension"/>-->
        <!--<attr name="mwScaleX" format="float"/>-->
        <!--<attr name="mwScaleY" format="float"/>-->
    <!--</declare-styleable>-->



    <attr name="item_count" format="integer"></attr>
    <declare-styleable name="ViewPagerIndicator01">
        <attr name="item_count" />
    </declare-styleable>

    <declare-styleable name="MyRatingBar">
        <attr name="base_of_width_or_height" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
        <attr name="step" format="float"/>
        <attr name="rating" format="float"/>
        <attr name="stars" format="integer"/>
    </declare-styleable>
    <!--popupbtn-->
    <declare-styleable name="popupbtn">
        <!-- 正常状态下的背景 -->
        <attr name="normalBg" format="reference" />
        <!-- 按下状态下的背景 -->
        <attr name="pressBg" format="reference" />
        <!-- 正常状态下的图标 -->
        <attr name="normalIcon" format="reference" />
        <!-- 按下状态下的图标 -->
        <attr name="pressIcon" format="reference" />
        <!-- 按钮的padding -->

    </declare-styleable>

    <declare-styleable name="XEditText">
        <attr name="x_separator" format="string"/>
        <attr name="x_disableClear" format="boolean"/>
        <attr name="x_clearDrawable" format="reference"/>
        <attr name="x_showPwdDrawable" format="reference"/>
        <attr name="x_hidePwdDrawable" format="reference"/>
        <attr name="x_disableEmoji" format="boolean"/>
        <attr name="x_togglePwdDrawableEnable" format="boolean"/>
        <attr name="x_pattern" format="string"/>
    </declare-styleable>
</resources>