<resources>

    <!-- Base application theme. -->

    <!--load-->
    <style name="load_pop_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:windowBackground">@color/no_color</item>
        <item name="android:backgroundDimEnabled">true</item>

        <!--<item name="android:windowAnimationStyle">@style/config_pop_anim</item>-->
    </style>

    <style name="AnimBottom">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <!--横向分割线-->
    <style name="background_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:color">@color/gray03</item>
    </style>

    <!--竖向分割线-->
    <style name="view_h">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:color">@color/gray03</item>
    </style>

    <style name="weekIndicatorStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ff25adff</item>
    </style>

    <style name="fiveRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/five_rating_bar</item>

    </style>

    <style name="ActionMenuTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Menu">
        <item name="android:textSize">@dimen/action_text_size</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="PhotoPickerTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/photopicker_background</item><!--设置显示图片的背景-->
        <item name="actionMenuTextAppearance">@style/ActionMenuTextStyle</item>
    </style>


    <style name="AnimHead" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/head_in</item>
        <item name="android:windowExitAnimation">@anim/head_out</item>
    </style>

    <style name="txt_titleStyle1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="problem_list_item">
        <item name="android:background">@drawable/ss</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingLeft">@dimen/x10</item>
        <item name="android:paddingRight">@dimen/x10</item>
    </style>

    <style name="problem_list_content_dim">
        <item name="android:layout_marginBottom">@dimen/x5</item>
    </style>

    <style name="problem_list_content01">
        <item name="android:textSize">@dimen/x12</item>
        <item name="android:textColor">#333333</item>
        <item name="android:layout_marginRight">@dimen/x5</item>
    </style>

    <style name="problem_list_content02">
        <item name="android:textSize">@dimen/x12</item>
        <item name="android:textColor">#999999</item>
        <item name="android:layout_marginRight">@dimen/x4</item>
    </style>
    <style name="MultiWaveHeader">
    </style>
    <style name="MultiWaveHeader.Wave">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
    </style>
    <style name="delete_pop_anim" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>
    <style name="meet_pop_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:windowBackground">@color/no_color</item>
        <!--<item name="android:backgroundDimEnabled">true</item>-->
        <item name="android:windowAnimationStyle">@style/delete_pop_anim</item>
    </style>

    <style name="head_pop_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/no_color</item>
        <item name="android:windowBackground">@color/no_color</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
</resources>
