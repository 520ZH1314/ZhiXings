            android:layout_marginRight="@dimen/x20"
            android:layout_marginEnd="@dimen/x20"
            android:textSize="@dimen/s12"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:background="#4ABBEF"
            android:id="@+id/textView43" />

    </RelativeLayout>
    <View
