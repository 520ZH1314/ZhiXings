<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ATDragView">
        <attr name="seek_bg_color" format="color" />
        <attr name="seek_pb_color" format="color" />
        <attr name="seek_ball_solid_color" format="color" />
        <attr name="seek_ball_stroke_color" format="color" />
        <attr name="seek_text_color" format="color" />
        <attr name="seek_text_size" format="dimension" />
    </declare-styleable>
    <style name="def_dragview">
        <item name="seek_bg_color">#e6e6e6</item>
        <item name="seek_pb_color">#8ace00</item>
        <item name="seek_ball_solid_color">#ffffff</item>
        <item name="seek_ball_stroke_color">#e6e6e6</item>
        <item name="seek_text_color">#666666</item>
        <item name="seek_text_size">12sp</item>
    </style>
    <declare-styleable name="SwitchView">
        <attr name="switch_bg_close_color" format="color" />
        <attr name="switch_bg_open_color" format="color" />
        <attr name="switch_strock_color" format="color" />
        <attr name="switch_ball_color" format="color" />
        <attr name="switch_ball_shadow_color" format="color" />
        <attr name="switch_bg_strock_width" format="dimension" />
    </declare-styleable>

    <style name="def_switch_view">
        <item name="switch_bg_close_color">#666666</item>
        <item name="switch_bg_open_color">#8ace00</item>
        <item name="switch_strock_color">#e6e6e6</item>
        <item name="switch_ball_color">#ffffff</item>
        <item name="switch_ball_shadow_color">#ff00ee</item>
        <item name="switch_bg_strock_width">2dp</item>
    </style>
</resources>